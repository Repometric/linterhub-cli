{
    "$schema": "./../linter.schema.json",
    "name": "VNU",
    "description": "The Nu Html Checker (v.Nu) is the backend of checker.html5.org, html5.validator.nu, and validator.w3.org/nu.",
    "url": "https://validator.github.io/validator/",
    "languages": [ "html" ],
    "extensions": [ "*.html", "*.htm", "*.xhtml", "*.xht" ],
    "license": "MIT",
    "areas": [ "syntax" ],
    "requirements": [ 
        { "manager": "system", "package": "java" }
    ],
    "version": {
        "local": "17.3.0"
    },
    "defaults": {
		"-jar": "vnu.jar"
        "--format": "xml",
        "": "**/**/*.js"
    }
}